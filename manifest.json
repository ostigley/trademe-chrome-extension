{
  "manifest_version": 2,
  "name": "Trademe Next",
  "version": "0.0.1",

  "description": "Lets a user click 'next' through trademe search results",
  "icons": {},

  "page_action": {
    "default_icon": "./icon.png",
    "default_title": "Trademe Next"
  },

  "content_scripts": [{
    "matches": ["*://www.trademe.co.nz/*"],
    "js": ["jquery.js", "trademe-next.js"],
    "run_at": "document_end"
  }],
  "permissions": [
    "tabs"
  ]
}
